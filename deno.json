{
  "compilerOptions": {
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "tasks": {
    "dev": "deno run -A --watch=src/static/,src/routes/ src/dev.ts",
    "cache": "deno cache --reload src/main.ts",
    "update": "deno run --allow-read --allow-env --allow-net scripts/update.ts",
    "check": "deno lint && deno fmt && deno check main.ts"
  },
  "lock": false,
  "lint": {
    "files": {
      "exclude": [
        "static",
        "docs",
        "resources"
      ]
    },
    "rules": {
      "tags": [
        "recommended"
      ],
      "include": [
        "ban-untagged-todo"
      ]
    }
  },
  "fmt": {
    "files": {
      "exclude": [
        "static",
        "docs",
        "resources"
      ]
    },
    "options": {
      "useTabs": true,
      "indentWidth": 4,
      "singleQuote": false,
      "proseWrap": "preserve"
    }
  },
  "imports": {
    "@/": "./src/",
    "$std/": "https://deno.land/std@0.171.0/",
    "dotenv": "https://deno.land/std@0.171.0/dotenv/load.ts",
    "$fresh/": "https://deno.land/x/fresh@1.2.0/",
    "fresh_seo": "https://deno.land/x/fresh_seo@0.2.1/mod.ts",
    "gfm": "https://deno.land/x/gfm@0.1.22/mod.ts",
    "tabler-icons/": "https://deno.land/x/tabler_icons_tsx@0.0.2/tsx/",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "supabase": "https://esm.sh/@supabase/supabase-js@2.1.3",
    "supabase-client": "https://esm.sh/@supabase/supabase-js@2.1.3?target=es2019",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.1.0",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "preact": "https://esm.sh/preact@10.15.1",
    "@twind/core": "https://esm.sh/@twind/core@1.1.3",
    "preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4",
    "preset-autoprefix": "https://esm.sh/@twind/preset-autoprefix@1.0.7",
    "ua-parser-js": "https://esm.sh/ua-parser-js@1.0.2",
    "imagescript": "https://esm.sh/imagescript@1.2.15"
  },
  "scopes": {}
}